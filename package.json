{
  "name": "file-tree",
  "version": "0.3.0",
  "private": true,
  "main": "dist/FileTree.esm.js",
  "module": "dist/FileTree.esm.js",
  "unpkg": "dist/FileTree.min.js",
  "browser": {
    "./sfc": "src/components/FileTree/index.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/FileTree.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/FileTree.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/FileTree.min.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "core-js": "^2.6.5",
    "sl-vue-tree": "^1.8.4",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.0",
    "rollup": "^1.11.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.1"
  },
  "description": "## Project setup ``` yarn install ```",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tokunaga1/file-tree.git"
  },
  "keywords": [
    "file",
    "tree",
    "view"
  ],
  "author": "tokuiti <tokuiti@gmail.com> (https://github.com/tokuiti)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tokunaga1/file-tree/issues"
  },
  "homepage": "https://github.com/tokunaga1/file-tree#readme"
}
